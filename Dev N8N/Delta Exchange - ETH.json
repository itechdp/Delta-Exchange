    {
      "parameters": {
        "jsCode": "const spot = $input.first().json.result.spot_price; // e.g., 108422\nconst step = 20;\n\n// Calculate strikes\nconst lowerStrike = Math.floor(spot / step) * step;\nconst upperStrike = Math.ceil(spot / step) * step;\n\n// All Time Slot data (with passed_hours, Run, etc.)\nconst timeSlots = $(\"Time is 5:30 AM?\").all();\n\n// Return each item properly in array format\nreturn timeSlots.map(item => {\n  return {\n    json: {\n      ...item.json,\n      lowerStrike,\n      upperStrike\n    }\n  };\n});\n"
      },
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        180,
